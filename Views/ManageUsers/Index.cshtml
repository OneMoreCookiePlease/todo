@model ManageUsersViewModel
@{
    ViewData["Title"] = "Manage users";
}
<h2>@ViewData["Title"]</h2>
<h3>Administrators</h3>
<table class="table">
    <thead>
        <tr>
            <td>Id</td>
            <td>Email</td>
        </tr>
    </thead>
    @foreach (var item in Model.Administrators)
    {
        <tr>
            <td>@item.Id</td>
            <td>@item.Email</td>
        </tr>
    }
</table>
<h3>Everyone</h3>
<table class="table">
    <thead>
        <tr>
            <td>Id</td>
            <td>Email</td>
        </tr>
    </thead>
    @foreach (var item in Model.Everyone)

    {
        <tr>
            <td>@item.Id</td>
            <td>@item.Email</td>
        </tr>
    }
</table>





